{
	"env": {
		"browser": true,
		"commonjs": true
	},
	"extends": "eslint:recommended",
	"globals": {
		"process": true,
		"$": true,
		"Tablesaw": true,
		"TablesawConfig": true
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"warn",
			"double"
		],
		"semi": [
			"error",
			"always"
		]
	}
}